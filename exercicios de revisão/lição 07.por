programa {
  funcao inicio() {
    inteiro v, c, t

    escreva("qual é a velocidade em km/h? ")
    leia(v)

    c = v/3.6

    escreva("A velocidade convertida em m/s é: ", c, "m/s")

    t = 434/v

    escreva("\nEm viagem de São Paulo até o Rio de Janeiro, o tempo médio para chegar é: ")

    escreva(t)

    escreva("hrs")
    
  }
}
